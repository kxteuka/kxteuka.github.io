{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "user-select"
        ]
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "meta[name=theme-color]"
        ]
      }
    ],
    "css-prefix-order": "off",
    "disown-opener": "off"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not ios_saf <= 18.5",
    "not safari <= 18.5"
  ]
}